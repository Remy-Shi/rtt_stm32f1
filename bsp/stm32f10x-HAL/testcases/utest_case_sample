#include <rtthread.h>
#include "utest.h"

static void sample_utest(void)
{
	uassert_true(1);
}

static rt_err_t sample_utest_init(void)
{
	return RT_EOK;
}

static rt_err_t sample_utest_cleanup(void)
{
	return RT_EOK;
}

static void sample_utestcase(void)
{
	UTEST_UNIT_RUN(sample_utest);
}
UTEST_TC_EXPORT(sample_utestcase, 		\
				"sample_tc",			\
				sample_utest_init, 		\
				sample_utest_cleanup,	\
				10);
